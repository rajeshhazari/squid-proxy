{
  "name": "web-squid-proxy",
  "version": "1.0.0",
  "description": "Squid Proxy as Docker With Angular Dahsboard to manage block lists domainas, url and acls.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run server:run\" \"ng serve  --host 0.0.0.0 --disable-host-check --port 4100 --proxy-config proxy.conf.json\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "test:client": "ng test --watch false",
    "e2e:client": "ng e2e",
    "server:run": "nodemon",
    "server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "private": true,
  "repository": {
    "url": "https://github.com/rajeshhazari/squid-proxy"
  },
  "keywords": [
    "proxy",
    "squid",
    "squid-proxy",
    "squidproxy",
    "squigGuard",
    "http"
  ],
  "author": "Rajesh Hazari",
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~9.0.2",
    "@angular/compiler": "~9.0.2",
    "@angular/core": "~9.0.2",
    "@angular/forms": "~9.0.2",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/platform-server": "^11.0.5",
    "@angular/router": "~9.0.2",
    "@types/mocha": "^8.2.0",
    "core-js": "^3.8.1",
    "express": "^4.17.1",
    "express-jwt": "^5.1.0",
    "express-session": "^1.17.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^7.3.0",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "rxjs": "~6.5.4",
    "shelljs": "0.8.4",
    "ts-helpers": "1.1.2",
    "tslib": "^1.10.0",
    "tslint": "^5.8.0",
    "uuid": "^3.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "~9.0.3",
    "@angular/compiler-cli": "~9.0.2",
    "@angular/language-service": "~9.0.2",
    "@types/express": "^4.17.9",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "nodemon": "^2.0.6",
    "protractor": "~5.4.3",
    "ts-node": "^8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
