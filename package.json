{
  "name": "web-squid-proxy",
  "version": "0.0.1",
  "description": "Squid Proxy as Docker With Angular Dahsboard to manage block lists domainas, url and acls.",
  "main": "server/app.js",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run server:run\" \"ng serve  --host 0.0.0.0 --disable-host-check --port 4100 --proxy-config proxy.conf.json\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "test:client": "ng test --watch false",
    "e2e:client": "ng e2e",
    "server:run": "nodemon",
    "server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "keywords": [
    "proxy",
    "squid",
    "squid-proxy",
    "squidproxy",
    "squigGuard",
    "http"
  ],
  "author": "Rajesh Hazari",
