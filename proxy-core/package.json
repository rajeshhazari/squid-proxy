{
  "name": "proxy-core",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "lint": "npm run lint:js && npm run lint:ejs",
    "lint:js": "eslint ./routes ./resources ",
    "lint:ejs": "find views -type f -iname '*.ejs' -exec bash -c \"./node_modules/.bin/ejslint '{}'\" \\;",
    "devstart": "nodemon ./bin/www",
    "dev": "DEBUG=proxy-core:* npm run devstart",
    "prod": "npm run production",
    "production": "cross-env nodemon ./bin/www"
  },
  "keywords": [
    "proxy",
    "squid",
    "squid-proxy",
    "squidproxy",
    "squigGuard",
    "http"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "bootstrap": "^5.0.0-beta1",
    "cookie-parse": "0.4.0",
    "cookie-parser": "~1.4.4",
    "cors": "2.8.5",
    "debug": "~2.6.9",
    "ejs": "^3.1.5",
    "ejs-lint": "^1.1.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "jquery": "^3.5.1",
    "morgan": "~1.9.1",
    "pbkdf2-password": "^1.2.1",
    "popper.js": "^1.16.1",
    "pug": "^2.0.0-beta11",
    "pug-client": "^1.0.2",
    "shelljs": "^0.8.4"
  }
}
